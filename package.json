{
  "name": "asr-demo",
  "version": "0.1.0",
  "private": true,
  "main": "./src/electron/index.js",
  "dependencies": {
    "antd": "^3.7.0",
    "archiver": "^2.1.1",
    "axios": "^0.18.0",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "form-data": "^2.3.2",
    "fs-extra": "3.0.1",
    "raf": "3.4.0",
    "react": "^16.4.1",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "resolve": "1.6.0",
    "uuid": "^3.3.2",
    "wavesurfer.js": "^2.0.6",
    "xlsx": "^0.13.2"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "dev": "NODE_ENV=development electron .",
    "build": "node scripts/build.js",
    "postbuild": "node scripts/compileElectron.js",
    "electron": "electron ./build",
    "package:linux": "electron-packager ./build --overwrite --platform=linux --arch=x64 --out=out",
    "package:win64": "electron-packager ./build --overwrite --platform=win32 --arch=x64 --out=out",
    "build-win64": "build --projectDir ./build --win --x64",
    "build-win32": "build --projectDir ./build --win --ia32",
    "test": "node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      [
        "env",
        {
          "targets": {
            "node": "current",
            "chrome": 60
          }
        }
      ]
    ],
    "plugins": [
      "syntax-dynamic-import",
      [
        "import",
        [
          {
            "libraryName": "antd",
            "style": "css"
          }
        ]
      ]
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "electron": "^2.0.5",
    "electron-builder": "^20.22.0",
    "electron-packager": "^12.1.0",
    "electron-prebuilt": "^1.4.13",
    "electron-winstaller": "^2.6.4",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "file-loader": "1.1.5",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "node-sass": "^4.9.2",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2"
  },
  "build": {
    "appId": "com.unisound.asr_demo",
    "productName": "asr_demo",
    "win": {
      "target": "nsis",
      "icon": "build\\resources\\icon\\icon.ico",
      "verifyUpdateCodeSignature": false
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "build\\resources\\icon\\icon.ico",
      "language": "2052"
    }
  }
}
